{
  "expo": {
    "name": "status",
    "slug": "status",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./sources/assets/icon.png",
    "splash": {
      "image": "./sources/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.status.luke",
      "config": {
        "googleMapsApiKey": "KEY"
      }
    },
    "android": {
      "package": "com.status.luke",
      "config": {
        "googleMaps": {
          "apiKey": "KEY"
        }
      }
    },
    "web": {
      "favicon": "./sources/assets/favicon.png"
    }
  }
}